<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ActionCircle">
        <attr name="scaleRatio" format="float" />
        <attr name="duration" format="integer" />
        <attr name="bgColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="menuIconRotateAngle" format="integer" />
        <attr name="menuIconHeight" format="dimension" />
        <attr name="menuIconWidth" format="dimension" />
        <attr name="shadowWidth" format="dimension" />
        <attr name="menuIcon" format="integer" />
        <attr name="firstActionIcon" format="integer" />
        <attr name="secondActionIcon" format="integer" />
        <attr name="thirdActionIcon" format="integer" />
        <attr name="fourthActionIcon" format="integer" />
    </declare-styleable>

    <declare-styleable name="RoundWaveView">
        <attr name="pointCount" format="integer" />
        <attr name="useAnimation" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Pinview">
        <attr name="pinBackground" format="reference" />
        <attr name="pinLength" format="integer" />
        <attr name="pinWidth" format="dimension" />
        <attr name="pinHeight" format="dimension" />
        <attr name="splitWidth" format="dimension" />
        <attr name="cursorVisible" format="boolean" />
        <attr name="password" format="boolean" />
        <attr name="forceKeyboard" format="boolean" />
        <attr name="hint" format="string" />
        <attr name="inputType" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>

    <attr name="inputType" format="enum">
        <enum name="text" value="0" />
        <enum name="number" value="1" />
    </attr>

</resources>